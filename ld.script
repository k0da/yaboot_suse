/* $Id$ */
OUTPUT_ARCH(powerpc:common)
ENTRY(_yaboot_start)
SECTIONS
{
	. = (64*1024);
	_start = .;
	.text	:
	{
		*(.text)
		*(.fixup)
		__relocate_start = .;
		*(.relocate_code)
		__relocate_end = .;
	}
	_etext = .;
	. = ALIGN(16);
	.data	:
	{
		*(.data*)
		*(.sdata*)
		*(.rodata*)
		__got2_start = .;
		*(.got2)
		__got2_end = .;
	}
	_edata = .;
	_yaboot_conf_start = .;
	.yaboot.conf :
	{
		*(.yaboot.conf)
	}
	_yaboot_conf_end = .;
	. = ALIGN(16);
	__bss_start = .;
	.bss	:
	{
		*(.bss)
		*(.sbss)
	}
	_end = .;
}
